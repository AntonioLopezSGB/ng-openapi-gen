{
  "name": "ng-openapi-gen",
  "version": "0.2.0",
  "license": "MIT",
  "author": "Cyclos development team",
  "description": "An OpenAPI 3 codegen for Angular 6+",
  "keywords": [
    "angular",
    "openapi",
    "codegen"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyclosproject/ng-openapi-gen.git"
  },
  "private": true,
  "bin": {
    "ng-openapi-gen": "lib/index.js"
  },
  "scripts": {
    "test": "jasmine-ts tsconfig.json --config=jasmine.json",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "compile": "tsc --project tsconfig.json && ncp \".npmignore\" dist && ncp \"LICENSE\" dist && ncp \"README.md\" dist && ncp \"templates\" dist && ncp \"lib/index.js\" dist/lib && replace-in-file \"\\\"private\\\": true\" \"\\\"private\\\": false\" dist/package.json",
    "build": "npm run tslint && npm run compile && npm test"
  },
  "dependencies": {
    "@loopback/openapi-v3-types": "^1.0.10",
    "@types/argparse": "^1.0.36",
    "@types/jsesc": "^0.4.29",
    "@types/json-schema": "^7.0.3",
    "@types/lodash": "^4.14.123",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.14.3",
    "argparse": "^1.0.10",
    "esm": "^3.2.20",
    "handlebars": "^4.1.1",
    "jsesc": "^2.5.2",
    "json-schema": "^0.2.3",
    "json-schema-ref-parser": "^5.1.3",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.3.4000"
  },
  "peerDependencies": {
    "@angular/core": ">=6.0.0",
    "rxjs": ">=6.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "ncp": "^2.0.0",
    "replace-in-file": "^3.4.4",
    "tslint": "^5.14.0",
    "typescript-parser": "^2.6.1"
  }
}
